{
  "basePaths": {
    "src": "./src",
    "dest": "./"
  },
  "paths": {
    "images": {
      "src": "./src/image/",
      "dest": "./image/",
      "sprite": {
        "svg": {
          "src": "./src/image/sprite/svg/",
          "output": "Website-Block-Xhtmltemplate-Svgsprite.all.all.html",
          "dest": "./modules/website/templates/"
        }
      }
    },
    "scripts": {
        "src": "./src/js/",
        "dest": "./js/"
    },
    "styles": {
        "src": "./src/style/",
        "dest": "./style/"
    },
    "sprite": {
        "src": "./src/sprite/*"
    },
    "fonts": {}
  },
  "plugins": {
    "autoprefixer": {
      "browsers": ["last 2 version", "safari 5", "ie 8", "ie 9", "opera 12.1", "ios 6", "android 4"]
    },
    "imagemin": {
      "formats": "*.{png,jpg,jpeg,gif}"
    }
  }, 
  "js": {
    "vendor": [
      "./src/vendor/browser-detection/src/browser-detection.js"
    ]
  }
}