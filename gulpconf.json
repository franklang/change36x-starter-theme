{
  "theme": "starter",
  "basePaths": {
    "src": "src",
    "dest": ""
  },
  "paths": {
    "images": {
      "src": "src/image/",
      "dest": "image/",
      "sprites": {
        "svg": {
          "src": "src/image/sprites/svg/",
          "output": "Website-Block-Xhtmltemplate-Svgsprite.all.all.html",
          "dest": "modules/website/templates/"
        }
      }
    },
    "scripts": {
        "src": "src/js/",
        "dest": "js/"
    },
    "styles": {
        "src": "src/style/",
        "dest": "style/"
    },
    "sprites": {
        "src": "src/sprite/*"
    },
    "fonts": {}
  },
  "clean": {
    "style": {
      "ignore": ["!style/layout.css", "!style/layout.xul.all.css", "!style/skin-variables.css", "!style/skin.css"]
    }
  },
  "plugins": {
    "autoprefixer": {
      "browsers": ["last 2 version", "safari 5", "ie 8", "ie 9", "opera 12.1", "ios 6", "android 4"]
    },
    "imagemin": {
      "formats": "*.{png,PNG,jpg,JPG,jpeg,JPEG,gif,GIF}"
    }
  }, 
  "css": {
    "vendor": [
      "src/vendor/jquery-ui/themes/blitzer/jquery-ui.min.css"
    ]
  },
  "js": {
    "vendor": [
      "src/vendor/bootstrap/js/popover.js"
    ]
  }
}