{
  "basePaths" : {
    "src": "./src",
    "dest": "./"
  },
  "paths" : {
    "images" : {
      "src": "./src/image/",
      "dest": "./image/",
      "sprite" : {
        "svg" : {
          "src": "./src/image/sprite/svg/",
          "output": "Website-Block-Xhtmltemplate-Svgsprite.all.all.html",
          "dest": "./modules/website/templates/"
        }
      }
    },
    "scripts" : {
        "src": "./src/js/",
        "dest": "./js/"
    },
    "styles" : {
        "src": "./src/style/",
        "dest": "./style/"
    },
    "sprite" : {
        "src": "./src/sprite/*"
    },
    "fonts" : {}
  },  
  "js" : {
    "vendor" : [
      "./src/vendor/browser-detection/src/browser-detection.js",
      "./src/vendor/foundation/js/foundation.js",
      "./src/vendor/modernizr/lib/metadata.js",
      "./src/vendor/jquery-mobile/js/events/navigate.js",
      "./src/vendor/jquery-mobile/js/events/touch.js",
      "./src/vendor/jquery-mobile/js/jquery.mobile.events.js" 
    ]
  }
}