<?xml version="1.0" encoding="UTF-8"?>
<change:templates xmlns:change="http://www.rbs.fr/change/1.0/schema" templateType="tableCells">
  <change:template id="tplTwoColumnsLeft">
    <div id="top">

      <change:templateblock type="modules_solrsearch_form" />
      <change:templateblock type="modules_website_ConfigurableMenu" __mode="tag" __tag="contextual_website_website_menu-main" __displayMode="scroll" __depth="2" />
      
      <div id="sidebarLeft">
        <change:content id="sidebarLeft-block"/>
      </div>
      <div id="content">
        <change:content id="content-block"/>
      </div>
    </div>
  </change:template>

  <change:template content-type="xul" id="tplTwoColumnsLeft">
    <vbox id="top">
      <hbox flex="12">
        <vbox id="sidebar" flex="3">
          <change:content id="sidebar-block"/>
        </vbox>
        <vbox id="content" flex="9">
          <change:content id="content-block"/>
        </vbox>
      </hbox>
    </vbox>
  </change:template>
</change:templates>

